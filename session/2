let SessionLoad = 1
let s:so_save = &g:so | let s:siso_save = &g:siso | setg so=0 siso=0 | setl so=-1 siso=-1
let v:this_session=expand("<sfile>:p")
silent only
silent tabonly
cd ~/
if expand('%') == '' && !&modified && line('$') <= 1 && getline(1) == ''
  let s:wipebuf = bufnr('%')
endif
set shortmess=aoO
badd +38 Google\ Drive/OmMahesh/Programming/Other/USACO/2021\ January/Silver\ \#1/main.cpp
badd +27 .config/nvim/UltiSnips/cpp.snippets
badd +9 main.in
badd +1 Makefile
badd +4 main2.cpp
badd +1 main.cpp
badd +4 .config/nvim/UltiSnips/snippets.snippets
badd +1 .config/nvim/plugged/vim-snippets/snippets/snippets.snippets
badd +1 .config/nvim/plugged/vim-snippets/snippets/simplemvcf.snippets
badd +1 .config/nvim/plugged/vim-snippets/snippets/_.snippets
badd +1 .config/nvim/main.cpp
badd +1 .config/nvim/main.in
badd +1 .config/nvim/Makefile
argglobal
%argdel
edit main.in
set splitbelow splitright
set nosplitbelow
wincmd t
set winminheight=0
set winheight=1
set winminwidth=0
set winwidth=1
argglobal
setlocal fdm=manual
setlocal fde=0
setlocal fmr={{{,}}}
setlocal fdi=#
setlocal fdl=0
setlocal fml=1
setlocal fdn=20
setlocal fen
silent! normal! zE
let &fdl = &fdl
let s:l = 1 - ((0 * winheight(0) + 14) / 28)
if s:l < 1 | let s:l = 1 | endif
keepjumps exe s:l
normal! zt
keepjumps 1
normal! 0
tabnext 1
if exists('s:wipebuf') && len(win_findbuf(s:wipebuf)) == 0&& getbufvar(s:wipebuf, '&buftype') isnot# 'terminal'
  silent exe 'bwipe ' . s:wipebuf
endif
unlet! s:wipebuf
set winheight=1 winwidth=20 winminheight=1 winminwidth=1 shortmess=filnxtToOFI
let s:sx = expand("<sfile>:p:r")."x.vim"
if filereadable(s:sx)
  exe "source " . fnameescape(s:sx)
endif
let &g:so = s:so_save | let &g:siso = s:siso_save
nohlsearch
doautoall SessionLoadPost
unlet SessionLoad
